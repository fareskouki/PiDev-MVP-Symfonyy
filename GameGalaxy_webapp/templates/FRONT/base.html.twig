	<script src="{{asset('FRONT/assets/js/vendor/bootstrap.bundle.min.js')}}"></script>
	<!-- custom select js -->
	<script src="{{asset('FRONT/assets/js/vendor/jquery.nice-select.min.js')}}"></script>
	<!-- lightcase js -->
	<script src="{{asset('FRONT/assets/js/vendor/lightcase.js')}}"></script><!-- wow js -->
	<script src="{{asset('FRONT/assets/js/vendor/wow.min.js')}}"></script>
	<!-- slick slider js -->
	<script src="{{asset('FRONT/assets/js/vendor/slick.min.js')}}"></script>
	<script src="{{asset('FRONT/assets/js/vendor/TweenMax.min.js')}}"></script>
	<!-- contact js -->
	<script src="{{asset('FRONT/assets/js/contact.js')}}"></script>
	<script src="{{asset('http://rexbd.net/html/sporcia/demo/assets/js/vendor/jquery.paroller.min.js')}}"></script>
	<script src="{{asset('FRONT/assets/js/app.js')}}"></script>
	{% block custom_javascripts %}
	<!-- Include or Override when needed --><!-- METIER SHARE js -->
	<script src="{{asset('share42.js')}}"> </script>
	{% endblock %}
</head>


<body>
	<!-- prealoader start  -->
	<div class="preloader">
		<div class="spinner">
			<div class="dot1"></div>
			<div class="dot2"></div>
		</div>
	</div>
	<!-- prealoader end -->
	<!-- scroll-to-top start -->
	<div class="scroll-to-top">
		<span class="scroll-icon">
			<i class="las la-arrow-up"></i>
		</span>
	</div>
	<!-- scroll-to-top end -->
	<!-- page-wrapper start -->  
	<div class="page-wrapper">
		<!-- login modal -->
		<div class="modal fade" id="loginModal" tabindex="1" role="dialog" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
				<div class="modal-content bg-transparent">
					<!-- account-area start -->
					<div class="modal-body">
						<div class="account-area">
							<h3 class="title mb-4">Bienvenue à GameGalaxy</h3>
							<form class="account-form">
								<div class="form-group">
									<label>VotreEmail
									</label>
									<input type="email" name="login_email" placeholder="Enter Your Email"
										class="form-control">
								</div>
								<div class="form-group">
									<label>Votre Mot de Passe
									</label>
									<input type="password" name="login_email" placeholder="Enter Your Password"
										class="form-control">
								</div>
								<div class="form-group">
									<p>Vous avez oublié votre mot de passe?
										<a href="#0">Réinitialisez votre mot de passe</a>
									</p>
								</div>
								<div class="text-center">
									<button type="submit" class="cmn-btn">Connexion</button>
								</div>
							</form>
							<div class="or-devider mt-5">
								<span>OR</span>
							</div>
							<div class="text-center mt-4">
								<p>Connectez-vous avec votre email personnel</p>
								<a href="#0" class="google-btn mt-4 mb-3"><img src="{{asset(image_dir ~ 'icon/google.png')}}"
										alt="image">Connexion avec Google</a>
								<p>Vous n'avez pas de compte?
									<a href="#0" data-dismiss="modal" data-toggle="modal"
										data-target="#signupModal">Créez un compte ici</a>
								</p>
							</div>
						</div>
						<!-- account-area end -->
					</div>
				</div>
			</div>
		</div>
		<!-- login modal end -->
		<!-- Sign Up modal -->
		<div class="modal fade" id="signupModal" tabindex="1" role="dialog" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
				<div class="modal-content bg-transparent">
					<div class="modal-body">
						<div class="account-area">
							<h3 class="title mb-4">Commençons la création de votre compte...</h3>
							<div class="text-center mt-4">
								<a href="#0" class="google-btn mt-4 mb-3"><img src="{{asset(image_dir ~ 'icon/google.png')}}"
										alt="image">Connectez-vous avec Google</a>
							</div>
							<div class="or-devider mt-4">
								<span>OR</span>
							</div>
							<p class="text-center mt-2 mb-4">Inscrivez-vous avec votre email personnel</p>
							<form class="account-form">
								<div class="form-group">
									<label>Email
									</label>
									<input type="email" name="login_email" placeholder="Enter Your Email"
										class="form-control">
								</div>
								<div class="text-center">
									<button type="submit" class="cmn-btn">Essayez maintenant</button>
								</div>
							</form>
							<p class="text-center mt-3">Vous disposez déjà d'un compte?<a href="#0" data-dismiss="modal"
									data-toggle="modal" data-target="#loginModal">Connexion</a>
							</p>
						</div>
						<!-- account-area end -->
					</div>
				</div>
			</div>
		</div>
		<!-- Sign Up modal end -->

		<!-- header-section start  -->
		<header class="header">
			<div class="header__bottom">
				<div class="container">
					<!-- navbar start  -->
					<nav class="navbar navbar-expand-xl align-items-center">
						<a class="site-logo site-title" href="index.html">
							<img src={{ asset(image_dir ~ 'logo.png' ) }} alt="site-logo">
							<span class="logo-icon">
								<i class="flaticon-fire"></i>
							</span>
						</a>
						<button class="navbar-toggler ml-auto" type="button" data-toggle="collapse"
							data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
							aria-expanded="false" aria-label="Toggle navigation">
							<span class="menu-toggle"></span>
						</button>
						<div class="collapse navbar-collapse" id="navbarSupportedContent">
							<ul class="navbar-nav main-menu ml-auto">
								<li class="menu_has_children">
									<a href="/home">Acceuil</a>

								</li>
								<li class="menu_has_children">
									<a href="/produit/front">Boutique</a>
									<ul class="sub-menu">
										<li>
											<a href="/cart">Mon Panier</a>
											<!-- BOUCLE POUR AFFICHER CATEGORIES -->
										</li>

									</ul>
								</li>
								<li class="menu_has_children">
									<a href="/blog/front">Blog</a>
									<ul class="sub-menu">
										<li>
											<a href="/blog/front">Tous les blogs</a>
										</li>
									</ul>
								</li>
								<li class="menu_has_children">
									<a href="/evenement/front">Evenements</a>
									<ul class="sub-menu">
										<li>
											<a href="/reservation/front">Mes Réservations</a>
										</li>
									</ul>
								</li>
							</ul>
							<div class="nav-right">
								{% if app.user %}
								<a href="/logout">Se déconnecter</a>
								{% else %}
								<a href="/login">Se connecter</a>
								{% endif %}
							</div>
							
							<!-- <div class="nav-right">
								<a href="#0" data-toggle="modal" data-target="#loginModal">Connexion</a> 
							</div>-->
						</div>
					</nav>
				</div>
				<!-- header__bottom end -->
		</header>
		<!-- header-section end  -->

		{% block body %}
		<!-- Always override this block  -->
		{% endblock %}

		<!-- footer section start  -->
		<footer class="footer-section bg_img" data-background="{{asset(image_dir ~ 'elements/footer.png')}}"> <!-- missing image  -->
			<!-- footer section container start -->
			<div class="container">
				<!--<div class="row">
					<div class="col-lg-12">
						 <div class="subscribe-area">
					<div class="obj"><img src="assets\images\elements\subscribe.png" alt="image"></div>
					<div class="subscribe-content">
					  <h2 class="title">Find Out About New Games!</h2>
					  <p>Subscribe to get updated on future game releases</p>
					  <p>
					  <form class="subscribe-form">
						<input type="email" name="subscribe_email" id="subscribe_email" placeholder="Enter Your Email">
						<button type="submit">Subscribe</button>
					  </form>
					</div>
				  </div>
					</div>
				</div>-->
				<div class="row footer-top mb-none-50">
					<div class="col-lg-3 col-sm-6 mb-50">
						<div class="footer-widget">
							<h3 class="footer-widget__title">ABOUT</h3>
							<ul class="short-links">
								<li><a href="#0">About Us</a></li>
								<li><a href="#0">Contact Us</a></li>
